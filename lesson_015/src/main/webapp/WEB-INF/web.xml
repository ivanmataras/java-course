         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <display-name>Items</display-name>

    <servlet>
        <servlet-name>UserActions</servlet-name>
        <servlet-class>ru.imataras.servlets.UserActions</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserActions</servlet-name>
        <url-pattern>/UserActions</url-pattern>
    </servlet-mapping>

</web-app>